{{!--
	Invisible dummy frame to redirect response answer
	to this to don't reload the page after request.
--}}
<iframe style="display: none" name="dummyframe" id="dummyframe"></iframe>

<p>
	<table border="1">
		<caption>All Discord servers, where i'm present!</caption>
		<tr>
			<th>Icon</th>
			<th>ID</th>
			<th>Name</th>
		</tr>
		{{#each chans}}
		<tr id="{{this.id}}">
			<td><img src="{{this.icon}}" alt="" style="border: inset;"></td>
			<td>{{this.id}}</td>
			<td>"{{this.name}}"</td>
		</tr>
		{{/each}}
	</table>
</p>
